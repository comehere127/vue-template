{
  "name": "vue-landing-page",
  "version": "1.2.0",
  "description": "GT template using VUE 2.0",
  "author": "tudhm <tudhm@vng.com.vn>",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:production": "vue-cli-service build",
    "build:development": "vue-cli-service build --mode dev",
    "build:staging": "vue-cli-service build --mode staging",
    "build:sandbox": "vue-cli-service build --mode sandbox",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "test:unit": "vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "element-ui"
  ],
  "dependencies": {
    "element-ui": "^2.11.1",
    "lodash": "^4.17.11",
    "nprogress": "0.2.0",
    "vue": "2.6.10",
    "vue-i18n": "^5.0.3",
    "vue-meta": "^1.5.5",
    "vue-resource": "^1.5.1",
    "vue-router": "3.0.2",
    "vuex": "3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/eslint-config-prettier": "3.0.0-rc.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.9.0",
    "plop": "2.3.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "string-replace-loader": "^2.1.1",
    "stylelint-config-css-modules": "1.2.x",
    "stylelint-config-prettier": "3.2.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.2.x",
    "stylelint-scss": "3.1.x",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
